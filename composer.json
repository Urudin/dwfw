{
    "name": "different/dwfw",
    "description": "Different Development Web FrameWork",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Kukel Attila",
            "email": "kukel.attila@different.hu"
        }
    ],
    "keywords": [
        "framework",
        "laravel",
        "backpack",
        "different"
    ],
    "minimum-stability": "stable",
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "require": {
        "php": ">=8.1",
        "backpack/crud": "^5.5",
        "backpack/filemanager": "^2",
        "backpack/settings": "3.0.*",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.5",
        "intervention/image": "^2.7",
        "jenssegers/agent": "^2.6",
        "laravel/framework": "^9.0",
        "laravel/ui": "^4.2",
        "lukeraymonddowning/honey": "^0.5.0",
        "spatie/laravel-honeypot": "^4.3.2",
        "spatie/laravel-permission": "^5.9.1"
    },
    "require-dev": {
        "backpack/generators": "^3.3",
        "laravel/tinker": "^v2.8.1",
        "mockery/mockery": "^1.5.1",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "files": [
            "src/app/Helpers/helpers.php"
        ],
        "psr-4": {
            "Different\\Dwfw\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Different\\Dwfw\\Tests\\": "tests"
        },
        "exclude-from-classmap": [
            "**/config"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Different\\Dwfw\\DwfwServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-update-cmd": [
            "@php artisan dwfw:upgrade --ansi"
        ]
    }
}
